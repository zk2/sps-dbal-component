{
    "name": "zk2/sps-dbal-component",
    "type": "library",
    "description": "SQL query builder for Doctrine DBAL",
    "homepage": "https://github.com/zk2/sps-dbal-component",
    "license": "MIT",
    "keywords": [
        "sql",
        "dbal",
        "query builder",
        "doctrine"
    ],
    "authors": [
        {
            "name": "Evgeniy Budanov",
            "email": "budanov.ua@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-pdo": "*",
        "doctrine/dbal": "^3.0 || ^4",
        "doctrine/common": "^3.0",
        "doctrine/collections": "^2.0"
    },
    "require-dev": {
        "ext-json": "*",
        "symfony/var-dumper": "^6.4",
        "phpunit/phpunit": "^9.5",
        "symfony/dotenv": "^6.4",
        "monolog/monolog": "^2.1",
        "firehed/dbal-logger": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Zk2\\SpsDbalComponent\\": "src/"
        },
        "exclude-from-classmap": [
            "/tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Zk2\\Tests\\": "tests/"
        }
    }
}
