{
    "name": "zk2/sps-dbal-component",
    "type": "library",
    "description": "SQL query builder for Doctrine DBAL",
    "homepage": "https://github.com/zk2/sps-dbal-component",
    "license": "MIT",
    "keywords": [
        "sql",
        "dbal",
        "query builder",
        "doctrine"
    ],
    "authors": [
        {
            "name": "Evgeniy Budanov",
            "email": "budanov.ua@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4,<8.0",
        "ext-pdo": "*",
        "doctrine/dbal": "^2.10|^3.0",
        "doctrine/common": "^2.13|^3.0"
    },
    "require-dev": {
        "ext-json": "*",
        "symfony/var-dumper": "^5.1",
        "phpunit/phpunit": "^9.2",
        "symfony/dotenv": "^5.1",
        "monolog/monolog": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "Zk2\\SpsDbalComponent\\": "src/"
        },
        "exclude-from-classmap": [
            "/tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
